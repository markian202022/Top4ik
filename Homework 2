class Animal:
    def __init__(self, name, age):
        self.name = name
        self.age = age

    def info(self):
        return f"Ім'я: {self.name}, Вік: {self.age} років"


class Dog(Animal):
    def __init__(self, name, age):
        super().__init__(name, age)


class Cat(Animal):
    def __init__(self, name, age):
        super().__init__(name, age)


my_dog = Dog("Філя", 8)
my_cat = Cat("Масік", 14)

print(my_dog.info())
print(my_cat.info())
